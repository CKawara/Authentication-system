cation()\n    useEffect(() => {\n        // grab url and put it in a key value format\n        const values = queryString.parse(location.search)\n        const state = values.state ? values.state: null\n        const code = values.code ? values.code : null\n        \n        console.log('state' + state);\n        console.log('code' + code);\n\n        if(state && code){\n            props.googleAuthenticate(state, code)\n        } else{\n            props.CheckAuthenticated()\n            props.load_user()\n        }\n    }, [location])\n\n    return (\n        <div>\n            <Navbar/>\n            {props.children}\n        </div>\n    )\n}\n\nexport default connect(null, {CheckAuthenticated, load_user, googleAuthenticate})(Layout)\n","C:\\Users\\user\\Desktop\\Auth_application\\client\\src\\pages\\Login.js",[],[],"C:\\Users\\user\\Desktop\\Auth_application\\client\\src\\components\\Navbar.js",[],[],"C:\\Users\\user\\Desktop\\Auth_application\\client\\src\\actions\\auth.js",[],[],"C:\\Users\\user\\Desktop\\Auth_application\\client\\src\\reducers\\index.js",[],[],"C:\\Users\\user\\Desktop\\Auth_application\\client\\src\\actions\\types.js",[],[],"C:\\Users\\user\\Desktop\\Auth_application\\client\\src\\reducers\\auth.js",["84"],[],"import { LOGIN_SUCCESS, LOGIN_FAIL, \n        LOAD_USER_SUCCESS, LOAD_USER_FAIL,  \n        AUTHENTICATED_SUCCESS, AUTHENTICATED_FAIL, \n        LOGOUT,\n        PASSWORD_RESET_SUCCESS,PASSWORD_RESET_FAIL,\n        PASSWORD_RESET_CONFIRM_SUCCESS,PASSWORD_RESET_CONFIRM_FAIL,\n        SIGNUP_SUCCESS,SIGNUP_FAIL,\n        ACTIVATION_SUCCESS,ACTIVATION_FAIL,\n        GOOGLE_AUTH_SUCCESS, GOOGLE_AUTH_FAIL,\n    } from \"../actions/types\";\n\nconst initialState = {\n    access: localStorage.getItem('access'),\n    refresh: localStorage.getItem('refresh'),\n    isAuthenticated: null,\n    user: null\n}\n\nexport default function(state = initialState, action) {\n    const { type, payload } = action;\n\n    switch(type) {\n        case AUTHENTI